[
  {
    "value": "User {user.name} log in {host.ip}",
    "conditions": [
      {
        "fields": "event.type",
        "value": "start"
      },
      {
        "field": "host.ip"
      },
      {
        "field": "user.name"
      }
    ]
  },
    {
    "value": "User {user.name} log out from {host.ip}",
    "conditions": [
      {
        "fields": "event.type",
        "value": "end"
      },
      {
        "field": "host.ip"
      },
      {
        "field": "user.name"
      }
    ]
  },
   {
    "value": "{event.reason}",
    "conditions": [
      {
        "field": "event.reason"
      }
    ]
  },
  {
    "value": " the event code {event.code}, event generate by the username {user.name}",
    "conditions": [
      {
        "field": "event.code"
      },
      {
        "field": "user.name"
      }

    ]
  },
  {
    "value": "event source address is {source.address}",
    "conditions": [
      {
        "field": "source.address"
      }
    ]
  },
  {
    "value": "{source.ip} requested {url.path}",
    "conditions": [
      {
        "field": "source.ip"
      },
      {
        "field": "url.path"
      }
    ]
  },
  {
  "value": "{source.ip} connect to {destination.ip}",
  "conditions": [
      {
        "field": "source.ip"
      },
      {
        "field": "destination.ip"
      }
    ]
  }

]
