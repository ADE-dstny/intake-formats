[
  {
    "value": "User {user.name} ran {process.command_line}",
    "conditions": [
      { "field": "action.type", "value": "CMD" },
      { "field": "user.name" },
      { "field": "process.command_line" }
    ]
  },
  {
    "value": "Authentication of user {user.name}",
    "conditions": [{ "field": "user.name" }]
  },
  {
    "value": "Authentication of user {user.name} from domain {user.domain}",
    "conditions": [{ "field": "user.name" }, { "field": "user.domain" }]
  },
  {
    "value": "HTTP server received a request on {url.original} by {source.ip} and responded with status {http.response.status_code}",
    "conditions": [
      { "field": "action.type", "value": "logger" },
      { "field": "url.original" },
      { "field": "source.ip" },
      { "field": "http.response.status_code" }
    ]
  },
  {
    "value": "Error with connection from {source.ip}:{source.port} to {destination.ip}:{destination.port}",
    "conditions": [
      { "field": "action.outcome", "value": "error" },
      { "field": "source.ip" },
      { "field": "source.port" },
      { "field": "destination.port" },
      { "field": "destination.ip" }
    ]
  },
  {
    "value": "Failed connection from {source.ip}:{source.port} to {destination.ip}:{destination.port}",
    "conditions": [
      { "field": "action.outcome", "value": "failed" },
      { "field": "source.ip" },
      { "field": "source.port" },
      { "field": "destination.port" },
      { "field": "destination.ip" }
    ]
  },
  {
    "value": "Connection from {source.ip}:{source.port} to {destination.ip}:{destination.port}",
    "conditions": [
      { "field": "source.ip" },
      { "field": "source.port" },
      { "field": "destination.port" },
      { "field": "destination.ip" }
    ]
  },
  {
    "value": "Error with connection from {source.ip} to {destination.ip}",
    "conditions": [
      { "field": "action.outcome", "value": "error" },
      { "field": "source.ip" },
      { "field": "destination.ip" }
    ]
  },
  {
    "value": "Failed connection from {source.ip} to {destination.ip}",
    "conditions": [
      { "field": "action.outcome", "value": "failed" },
      { "field": "source.ip" },
      { "field": "destination.ip" }
    ]
  },
  {
    "value": "Connection from {source.ip} to {destination.ip}",
    "conditions": [{ "field": "source.ip" }, { "field": "destination.ip" }]
  }
]
